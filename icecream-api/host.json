{
    "version": "2.0",
    "logging": {
        "fileLoggingMode": "always",
        "logLevel": {
            "default": "Information",
            "Host.Results": "Error",
            "Function": "Error",
            "Host.Aggregator": "Trace"
        },
        "aggregator": {
            "batchSize": 1000,
            "flushTimeout": "00:00:30"
        },
        "applicationInsights": {
            "samplingExcludedTypes": "Request",
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond" : 20
            }
        }
    }
}
